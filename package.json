{
    "name": "gm-compat",
    "version": "0.1.2",
    "description": "Portable monkey-patching for userscripts",
    "author": "chocolateboy",
    "repository": "https://github.com/chocolateboy/gm-compat",
    "license": "Artistic-2.0",
    "main": "index.js",
    "jsdelivr": "index.min.js",
    "scripts": {
        "build": "terser --ecma 2015 --compress reduce_vars=false --mangle -o index.min.js < index.js",
        "build:doc": "toc-md README.md",
        "prepublishOnly": "run-s build build:doc"
    },
    "files": [
        "index.js",
        "index.min.js"
    ],
    "devDependencies": {
        "markdown-toc": "^1.2.0",
        "npm-run-all": "^4.1.5",
        "terser": "^5.5.0",
        "toc-md-alt": "^0.4.1"
    },
    "keywords": [
        "clone-into",
        "export-function",
        "greasemonkey",
        "shim",
        "tampermonkey",
        "unsafe-window",
        "userscript",
        "userscripts",
        "violentmonkey",
        "wrapped-js-object"
    ]
}
