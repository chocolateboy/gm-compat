{
    "name": "gm-compat",
    "version": "0.1.1",
    "description": "Portable monkey-patching for userscripts",
    "author": "chocolateboy",
    "repository": "https://github.com/chocolateboy/gm-compat",
    "license": "Artistic-2.0",
    "main": "index.js",
    "jsdelivr": "index.min.js",
    "scripts": {
        "build:old": "terser --ecma 2015 --compress reduce_vars=true --mangle -o index.min.js < index.js",
        "build": "terser --ecma 2015 --compress reduce_vars=false --mangle -o index.min.js < index.js",
        "doctoc": "markdown-toc --bullets=- --maxdepth 3 -i README.md"
    },
    "files": [
        "index.js",
        "index.min.js"
    ],
    "devDependencies": {
        "markdown-toc": "^1.2.0",
        "terser": "^5.3.0"
    },
    "keywords": [
        "clone-into",
        "export-function",
        "greasemonkey",
        "shim",
        "tampermonkey",
        "unsafe-window",
        "userscript",
        "userscripts",
        "violentmonkey",
        "wrapped-js-object"
    ]
}
